        "#@title Factorial PROGRAM\n",
        "n=int(input(\"Enter the number:\"))\n",
        "fact=1\n",
        "for i in range(1,n):\n",
        "  i=i+1\n",
        "  fact=fact*i\n",
        "print(\"factorial of\",n,\":\",fact)"
      ],
